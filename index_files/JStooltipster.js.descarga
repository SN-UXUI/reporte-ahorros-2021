$(document).ready(function() 
{
		$("[title]").tooltipster
		({
				theme: ["tooltipster-noir", "tooltipster-noir-customized"],      	
      	animation: "grow",
      	delay: 2000,
      	animationDuration : 20,
      	side: "top",
      	arrow: true,
      	minWidth: 0,
      	onlyOne:true      	
		});
});